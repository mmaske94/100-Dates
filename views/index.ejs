<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>Index</title>
</head>
<body>


  <h1>Penguin</h1>
  <a href="/users"><button id="buttonEdit">Profile</button></a><br>
  <a href="/users/login"><button id="buttonEdit">Login</button></a>
  <div class="buttonIndex">
    <a href="/dates/new"><button id="buttonIndex">+</button></a>
  </div>

      <div class="gridContainer">
        <%activities.forEach(function(activity){%>
               
                 <div class = "imagesIndex">
                  <div class = "squareIndex"></div>
                  <span class ='text'>
                    <h3 class = 'h3Index'><a class = 'link' href="/dates/<%=activity._id%>"><%=activity.nameOfDate%></a></h3>
                  </span> 
                </div>
                   
               

        <%}) %>
      </div>     
</body>
</html>